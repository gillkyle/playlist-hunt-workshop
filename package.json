{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "hasura-init": "hasura init",
    "hasura-console": "cd hasura && hasura console",
    "export-migrations": "cd hasura && hasura migrate create finished --from-server --endpoint https://playlist-hunt.herokuapp.com/",
    "export-meta": "cd hasura && hasura metadata export --endpoint https://playlist-hunt.herokuapp.com/",
    "migrate": "cd hasura && hasura migrate apply --endpoint https://playlist-hunt-workshop.herokuapp.com/",
    "metadata": "cd hasura && hasura metadata apply --endpoint https://playlist-hunt-workshop.herokuapp.com/",
    "hasura-setup": "cd hasura && hasura migrate apply --endpoint ${npm_config_endpoint} && hasura metadata apply --endpoint ${npm_config_endpoint}"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@auth0/auth0-spa-js": "1.6.3",
    "@chakra-ui/core": "^0.7.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@theme-ui/presets": "^0.3.0",
    "apollo-boost": "^0.4.7",
    "emotion-theming": "^10.0.27",
    "formik": "^2.1.4",
    "gatsby": "^4.25.7",
    "gatsby-plugin-chakra-ui": "^0.1.4",
    "graphql-tag": "^2.10.3",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-use-auth": "^0.5.3"
  },
  "devDependencies": {
    "hasura-cli": "^1.2.2",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
